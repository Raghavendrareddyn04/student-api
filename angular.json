    "test-bank-client": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "less"
        }
      },
      "root": "apps/test-bank-client",
      "sourceRoot": "apps/test-bank-client/src",
      "prefix": "plato-test-bank",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/test-bank-client",
            "index": "apps/test-bank-client/src/index.html",
            "main": "apps/test-bank-client/src/main.ts",
            "polyfills": "apps/test-bank-client/src/polyfills.ts",
            "tsConfig": "apps/test-bank-client/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/test-bank-client/src/favicon.ico",
              "apps/test-bank-client/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/test-bank-client/src/custom-theme.scss",
              "apps/test-bank-client/src/styles.less"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "aws-sdk",
              "chart",
              "ngrx-store-persist"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/test-bank-client/src/environments/environment.ts",
                  "with": "apps/test-bank-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "30mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "500kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test-bank-client:build",
            "proxyConfig": "apps/test-bank-client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "test-bank-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-bank-client:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/test-bank-client/tsconfig.app.json",
              "apps/test-bank-client/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/test-bank-client/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/test-bank-client/jest.config.js",
            "passWithNoTests": true
          }
